{
  "name": "Magic Goat Theme",
  "type": "dark",
  "colors": {
    "activityBarBadge.background": "#4b6d60",
    "activityBar.background": "#212225",
    "activityBar.foreground": "#a6accd",
    "activityBar.border": "#27282c",
    "breadcrumb.foreground": "#393f4e",
    "editor.background": "#252629",
    "editor.foreground": "#eeffff",
    "editorCursor.foreground": "#5c17fc",
    "editorHoverWidget.background": "#282c34",
    "editorHoverWidget.border": "#ffffff10",
    "editorLineNumber.activeForeground": "#754b4b",
    "editorLineNumber.foreground": "#35333b",
    "editor.selectionBackground": "#717cb450",
    "input.background": "#2a2b2e",
    "input.foreground": "#8371a0",
    "input.placeholderForeground": "#545158",
    "sideBar.background": "#212225",
    "sideBar.foreground": "#74747a",
    "sideBarTitle.foreground": "#858585",
    "statusBar.background": "#212225",
    "statusBar.debuggingBackground": "#422b3b",
    "statusBar.noFolderBackground": "#212225"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#616161"
      }
    },
    {
      "name": "Comment punctuation",
      "scope": ["punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#505050"
      }
    },
    {
      "name": "Variables, Block Level Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#e1e1f3"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#c3c8ec"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#caa9ff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff5353"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#00ff4c"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#cf5bb2"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#b280bd"
      }
    },
    {
      "name": "Punctuation 1",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#dbe27c"
      }
    },
    {
      "name": "Punctuation 2",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#8d64c4"
      }
    },
    {
      "name": "Punctuation 3",
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#EBEA8B"
      }
    },
    {
      "name": "Meta tag",
      "scope": ["meta.tag"],
      "settings": {
        "foreground": "#ffb0b0"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#7dc7bb"
      }
    },
    {
      "name": "Entity, Function, Class, Support, Special Method",
      "scope": [
        "entity.name",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "support.type",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#74a8e4"
      }
    },
    {
      "name": "Entity 2",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#a099f8"
      }
    },
    {
      "name": "Support class",
      "scope": ["support.class"],
      "settings": {
        "foreground": "#76a1b9"
      }
    },
    {
      "name": "Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b2b993"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#a14fff"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#76aa92"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#9ace84"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#8071d6"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#717bd6"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82ff82"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#baa5d4"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f5cc7f"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#f5cc7f"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#f8946c"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators 1",
      "scope": [
        "meta.decorator variable.other.property",
        "meta.decorator variable.other.object",
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6bce80"
      }
    },
    {
      "name": "Decorators 2",
      "scope": ["meta.decorator variable.other.readwrite"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#85c0ca"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
